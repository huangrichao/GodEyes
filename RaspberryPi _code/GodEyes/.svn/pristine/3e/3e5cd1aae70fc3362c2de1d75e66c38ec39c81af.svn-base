@charset "UTF-8";

// feedback variables
// --------------------------------------------------

////
/// @module feedback: 反馈
/// @tag  feedback
/// @category component
/// @family notice
/// @varPrefix $feedback-
/// @classPrefix {prefix}-feedback
/// @order {"size/bounding":10,"size/title":11,"size/message":12,"statement/normal":10,"statement/normal/bounding":100,"statement/normal/title":101,"statement/normal/message":102}
////

// Size:Large
// ----------------------------------------

/// padding (t)
/// @namespace size/bounding
$feedback-size-l-padding-top: $s-5 !default;
/// padding (r)
/// @namespace size/bounding
$feedback-size-l-padding-right: $s-5 !default;
/// padding (b)
/// @namespace size/bounding
$feedback-size-l-padding-bottom: $s-5 !default;
/// padding (l)
/// @namespace size/bounding
$feedback-size-l-padding-left: $s-5 !default;
/// border width
/// @namespace size/bounding
$feedback-size-l-border-width: $line-2 !default;
/// padding (l)
/// @namespace size/title
$feedback-size-l-title-padding-left: $s-3 !default;
/// padding (r)
/// @namespace size/title
$feedback-size-l-title-padding-right: $s-4 !default;
/// text
/// @namespace size/title
$feedback-size-l-title-font: $font-size-subhead !default;
/// padding (l)
/// @namespace size/message
$feedback-size-l-content-padding-left: $s-3 !default;
/// padding (r)
/// @namespace size/message
$feedback-size-l-content-padding-right: $s-4 !default;
/// margin (t)
/// @namespace size/message
$feedback-size-l-content-margin-top: $s-2 !default;
/// text
/// @namespace size/message
$feedback-size-l-content-font: $font-size-body-1 !default;
/// icon
/// @namespace size/message
$feedback-size-l-icon: $icon-xl !default;
// unconfigurable
$feedback-size-l-padding: $feedback-size-l-padding-top $feedback-size-l-padding-right $feedback-size-l-padding-bottom $feedback-size-l-padding-left !default;
$feedback-size-l-title-padding: 0 $feedback-size-l-title-padding-right 0 $feedback-size-l-title-padding-left + $feedback-size-l-icon !default;
$feedback-size-l-content-padding: 0 $feedback-size-l-content-padding-right 0 $feedback-size-l-content-padding-left + $feedback-size-l-icon !default;

// Size:Medium
// ----------------------------------------

/// padding (t)
/// @namespace size/bounding
$feedback-size-m-padding-top: $s-3 !default;
/// padding (r)
/// @namespace size/bounding
$feedback-size-m-padding-right: $s-3 !default;
/// padding (b)
/// @namespace size/bounding
$feedback-size-m-padding-bottom: $s-3 !default;
/// padding (l)
/// @namespace size/bounding
$feedback-size-m-padding-left: $s-3 !default;
/// border width
/// @namespace size/bounding
$feedback-size-m-border-width: $line-1 !default;
/// padding (l)
/// @namespace size/title
$feedback-size-m-title-padding-left: $s-2 !default;
/// padding (r)
/// @namespace size/title
$feedback-size-m-title-padding-right: $s-3 !default;
/// text
/// @namespace size/title
$feedback-size-m-title-font: $font-size-body-1 !default;
/// padding (l)
/// @namespace size/message
$feedback-size-m-content-padding-left: $s-2 !default;
/// padding (r)
/// @namespace size/message
$feedback-size-m-content-padding-right: $s-3 !default;
/// margin (t)
/// @namespace size/message
$feedback-size-m-content-margin-top: $s-1 !default;
/// text
/// @namespace size/message
$feedback-size-m-content-font: $font-size-body-1 !default;
/// icon
/// @namespace size/message
$feedback-size-m-icon: $icon-s !default;
// unconfigurable
$feedback-size-m-padding: $feedback-size-m-padding-top $feedback-size-m-padding-right $feedback-size-m-padding-bottom $feedback-size-m-padding-left !default;
$feedback-size-m-title-padding: 0 $feedback-size-m-title-padding-right 0 $feedback-size-m-title-padding-left + $feedback-size-m-icon !default;
$feedback-size-m-content-padding: 0 $feedback-size-m-content-padding-right 0 $feedback-size-m-content-padding-left + $feedback-size-m-icon !default;

// Statement:Common
// ----------------------------------------

/// corner
/// @namespace statement/normal/bounding
$feedback-border-radius: $corner-0 !default;
/// border style
/// @namespace statement/normal/bounding
$feedback-border-style: $line-solid !default;

// Statement:Success
// ----------------------------------------

/// background
/// @namespace statement/normal/bounding
$feedback-success-color-bg-inline: $color-success-1 !default;
/// background
/// @namespace statement/normal/bounding
$feedback-success-color-bg-addon: $color-transparent !default;
/// background
/// @namespace statement/normal/bounding
$feedback-success-color-bg-toast: $color-white !default;
/// shadow
/// @namespace statement/normal/bounding
$feedback-success-shadow-toast: $shadow-3 !default;
/// border color
/// @namespace statement/normal/bounding
$feedback-success-color-border-inline: $color-transparent !default;
/// border color
/// @namespace statement/normal/bounding
$feedback-success-color-border-toast: $color-transparent !default;
/// text
/// @namespace statement/normal/title
$feedback-success-color-title: $color-text1-4 !default;
/// text
/// @namespace statement/normal/message
$feedback-success-color-content: $color-text1-3 !default;
/// icon
/// @namespace statement/normal/message
$feedback-success-color-icon: $color-success-2 !default;

